{
    "name": "TabCrusher",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Crush your tabs",
    "permissions": [
        "tabs",
        "contextMenus",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["script/main.js"],
        "persistent": true
    }
}
